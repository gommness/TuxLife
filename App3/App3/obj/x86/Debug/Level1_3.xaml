        <Image x:ConnectionId='5' x:Name="floor_Copy2" HorizontalAlignment="Left" Height="64" Margin="644,521,0,0" VerticalAlignment="Top" Width="174" Source="Assets/platform.png"/>
        <Image x:ConnectionId='6' x:Name="floor_Copy3" HorizontalAlignment="Left" Height="64" Margin="823,521,0,0" VerticalAlignment="Top" Width="174" Source="Assets/platform.png"/>
        <Image x:ConnectionId='7' x:Name="floor_Copy4" HorizontalAlignment="Left" Height="64" Margin="1002,521,0,0" VerticalAlignment="Top" Width="174" Source="Assets/platform.png"/>
